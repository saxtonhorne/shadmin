<%= render partial: 'breadcrumbs', locals: { parent_title: 'Admin Users', resource: @admin_user } %>

<ul class="controls">
  <li><%= link_to 'Edit', shadmin.edit_admin_user_path(@admin_user) %></li>
  <li><%= link_to 'Delete', shadmin.admin_user_path(@admin_user), method: :delete, data: { confirm: 'Are you sure you want to delete this admin user?' } %>
  </li>
</ul>

<dl>
  <dt>ID</dt>
  <dd><%= @admin_user.id %></dd>
</dl>

<dl>
  <dt>Email</dt>
  <dd><%= @admin_user.email %></dd>
</dl>

<br><br>
