<%= form_for admin_admin_user_path(@admin_user) do |f| %>

	<%= f.label :email %>
	<%= f.text_field :email %>

	<br><br>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<br><br>

	<%= f.label :password_confirmation %>
	<%= f.password_field :password_confirmation %>

	<br><br>

	<%= f.submit 'Update' %>

<% end %>